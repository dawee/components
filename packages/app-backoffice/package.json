{
  "name": "app-backoffice",
  "version": "0.0.0",
  "description": "Brand backoffice",
  "main": "lib/index.js",
  "scripts": {
    "dev": "npm run build -- --watch & npm run start & wait",
    "build": "BABEL_ENV=es webpack",
    "server": "BABEL_ENV=commonjs node src/index.js",
    "start": "npm run build && npm run server",
    "test": "npm run lint && npm run nyc",
    "lint": "eslint src/ *.js *.json --ext .js,.json",
    "ava": "BABEL_ENV=commonjs ava",
    "nyc": "BABEL_ENV=commonjs nyc npm run ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoorpAcademy/app-brand.git"
  },
  "bugs": {
    "url": "https://github.com/CoorpAcademy/app-brand/issues"
  },
  "homepage": "https://github.com/CoorpAcademy/app-brand#readme",
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^2.0.0",
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "coveralls": "^2.11.13",
    "eslint": "^3.5.0",
    "history": "^3.2.1",
    "nyc": "^8.3.0",
    "redux-api-middleware": "^1.0.2"
  },
  "dependencies": {
    "@coorpacademy/components": "3.0.0-beta.3-alpha.be2dd1d7",
    "@coorpacademy/redux-history": "3.0.0-beta.3-alpha.be2dd1d7",
    "@coorpacademy/treantjs-core": "3.0.0-beta.3-alpha.be2dd1d7",
    "@coorpacademy/treantjs-engine-virtual-dom": "3.0.0-beta.3-alpha.be2dd1d7",
    "adaro": "^1.0.4",
    "autoprefixer": "^6.4.1",
    "body-parser": "^1.15.2",
    "cacheme": "0.0.5",
    "compression-webpack-plugin": "^0.3.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "lodash": "^4.15.0",
    "morgan": "^1.7.0",
    "redux": "^3.6.0",
    "rxjs": "^5.0.0-beta.12",
    "webpack": "^2.1.0-beta.25",
    "winston": "^2.2.0",
    "winston-logmatic": "^0.2.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**"
    ],
    "exclude": [
      "public/**/*",
      "src/**/test/**/*"
    ],
    "require": [
      "babel-core/register"
    ],
    "all": true
  }
}
