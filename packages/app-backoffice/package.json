{
  "name": "app-backoffice",
  "version": "0.0.0",
  "description": "Brand backoffice",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack",
    "start": "node src/index.js",
    "test": "npm run lint && npm run nyc",
    "lint": "eslint src/ *.js *.json --ext .js,.json",
    "ava": "ava",
    "nyc": "nyc ava --reporter=text --reporter=lcov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoorpAcademy/app-brand.git"
  },
  "bugs": {
    "url": "https://github.com/CoorpAcademy/app-brand/issues"
  },
  "homepage": "https://github.com/CoorpAcademy/app-brand#readme",
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^2.0.0",
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "coveralls": "^2.11.13",
    "eslint": "^3.5.0",
    "history": "^3.2.1",
    "nyc": "^8.3.0",
    "redux-api-middleware": "^1.0.2",
    "webpack": "^2.1.0-beta.21",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "@coorpacademy/components": "^2.2.11",
    "adaro": "^1.0.4",
    "body-parser": "^1.15.2",
    "cacheme": "0.0.5",
    "express": "^4.14.0",
    "lodash": "^4.15.0",
    "morgan": "^1.7.0",
    "redux": "^3.6.0",
    "rxjs": "^5.0.0-beta.12",
    "winston": "^2.2.0",
    "winston-logmatic": "^0.2.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/test/**"
    ],
    "require": [
      "babel-core/register"
    ],
    "all": true
  }
}
